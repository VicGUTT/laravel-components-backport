{
    "name": "vicgutt/laravel-components-backport",
    "description": "Backporting Laravel 7+ Blade components to Laravel 5.8+",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/VicGUTT/laravel-components-backport",
    "keywords": [
        "laravel",
        "blade",
        "component",
        "backport"
    ],
    "authors": [
        {
            "name": "Victor Gutt",
            "email": "guttvictor@yahoo.fr",
            "homepage": "https://victorgutt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "nesbot/carbon": "^2.41",
        "symfony/console": "^4.4|^5.0",
        "illuminate/console": "5.8.*|^6",
        "illuminate/support": "5.8.*|^6"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "VicGutt\\ComponentBackport\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VicGutt\\ComponentBackport\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VicGutt\\ComponentBackport\\Providers\\ComponentBackportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": []
}
